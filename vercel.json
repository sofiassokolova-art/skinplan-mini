{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/broadcasts?secret=CRON_SECRET_PLACEHOLDER",
      "schedule": "*/1 * * * *"
    }
  ]
}